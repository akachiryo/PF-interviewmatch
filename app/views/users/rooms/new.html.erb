 <%= form_with model: @room,url: users_rooms_path, local: true do |f| %>
    <div class="form-inline">
      <%= f.label :title, "ルーム名" %>
      <%= f.text_field :title %>
    </div>
     <div class="row mt-3">
       <div class="col-3"></div>
       <div class="col-2 fs-5"><%= f.label:time_tags, "時間指定" %></div>
       <div class="col-5"><%= f.collection_select(:time_tag_id, @time_tags, :id, :span, :include_blank => "--選択してください--") %></div>
     </div>
      <div class="row mt-3">
       <div class="col-3"></div>
       <div class="col-2 fs-5"><%= f.label:ocuupation_tags, "職種" %></div>
       <div class="col-5"><%= f.collection_select(:ocuupation_tag_id, @ocuupation_tags, :id, :name, :include_blank => "--選択してください--") %></div>
     </div>
  　<div class="form-inline">
      <%= f.label :content, "ルーム説明"%>
      <%= f.text_field :content %>
    </div>
    <%= f.submit "ルーム作成", class: "btn btn-success" %>
  <% end %>